#include <iostream>
using namespace std;
void swap(int *a, int *b){
	int temp;
	temp = *a;
	*a = *b;
	*b = temp;
}
int main() {


	int m, n;
	setlocale(LC_ALL, "Russian");
	cout << "Ввеедите m" << endl;
	cin >> m ;
	cout << "Введите n" << endl;
	cin >> n;
	int *nums1 = new int[m + n];
	
	cout << "Введите массив 1" << endl;
	for (int i = 0; i < m; i++) {
		cin >> nums1[i];
	}
	for (int i = m; i < m + n; i++) {
		nums1[i] = 0;
	}
	int *nums2 = new int[n];
	cout << "Введите массив 2" << endl;
	for (int i = 0; i < n; i++) {
		cin >> nums2[i];
	}
	
	
	
	int i = m - 1;
	int j = n - 1;
	int k = m + n - 1;

	while (j >= 0) {
		if (i >= 0 && nums1[i] > nums2[j]) {
			nums1[k--] = nums1[i--];
		}
		else {
			nums1[k--] = nums2[j--];
		}
	}
	
	for (int i = 0; i < m + n; i++) {
		cout << nums1[i] << " ";
	}
	cout << endl;
	delete [] nums1;
	delete[] nums2;
	return 0;
}
